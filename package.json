{
  "name": "instagram-clone",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:css": "postcss src/styles/tailwind.css -o src/styles/app.css",
    "watch:css": "postcss src/styles/tailwind.css -o src/styles/app.css --watch",
    "dev:start": "sleep 5 && vite --host",
    "dev": "run-p watch:css dev:start",
    "build": "run-s build:css vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "autoprefixer": "^10.4.13",
    "date-fns": "^2.29.3",
    "firebase": "^9.15.0",
    "postcss": "^8.4.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loading-skeleton": "^3.1.0",
    "react-router-dom": "^6.5.0",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0",
    "prop-types": "^15.8.1",
    "vite": "^4.0.0"
  }
}
